<%= turbo_frame_tag bookmark do %>
  <%= link_to bookmark.title, folder_bookmark_path(folder, bookmark), class: 'btn btn-warning btn-sm btn-block me-2',
  data: {turbo_frame: '_top'}%>
  <%= link_to :Edit, edit_folder_bookmark_path(folder, bookmark), class: 'btn btn-primary' %>
  <%= link_to :Delete, folder_bookmark_path(folder, bookmark), class: "btn btn-danger",
              data: { turbo_method: :delete } %>
<% end %>